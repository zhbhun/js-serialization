{
  "name": "js-serialization",
  "version": "0.0.1",
  "description": "js serializer which support undefined, NaN, Infinity, Date...",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:es": "tsc --project tsconfig.json --outDir es",
    "build:lib": "tsc --project tsconfig.lib.json --outDir lib",
    "build:types": "tsc --project tsconfig.types.json --outDir es"
  },
  "keywords": [
    "js",
    "serializer"
  ],
  "author": "zhbhun",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.2"
  }
}
